{
  "name": "theozebua/oze-framework-core",
  "description": "Just a simple mvc framework for beginners.",
  "keywords": [
    "teecoder",
    "oze",
    "mvc",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "psr/container": "2.0.x-dev",
    "vlucas/phpdotenv": "5.5.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "10.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "OzeFramework\\": "src/OzeFramework/"
    },
    "files": [
      "src/OzeFramework/View/helpers.php",
      "src/OzeFramework/Env/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OzeFramework\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
